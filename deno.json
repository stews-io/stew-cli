{
  "version": "0.0.0",
  // note: @jsxImportSource pragma seems to be needed for compile to work
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm/preact"
  },
  "fmt": {
    "useTabs": false,
    "singleQuote": true,
    "semiColons": true,
    "indentWidth": 2
  },
  "imports": {
    "deno/std/": "https://deno.land/std@0.200.0/",
    "deno/x/emit/": "https://deno.land/x/emit@0.27.0/",
    "deno/x/zod/": "https://deno.land/x/zod@v3.22.2/",
    "npm/es-iife": "https://esm.sh/es-iife@0.2.0",
    "npm/acorn": "https://esm.sh/acorn@8.10.0",
    "npm/preact": "https://esm.sh/preact@10.17.1",
    "npm/preact/jsx-runtime": "https://esm.sh/preact@10.17.1/jsx-runtime",
    "npm/preact-render-to-string": "https://esm.sh/preact-render-to-string@6.2.1?deps=preact@10.17.1"
  },
  "tasks": {
    "runCommand": "deno run --allow-env --allow-read --allow-net --deny-write ./source/main.tsx",
    "compileCli": "deno compile --allow-env --allow-read --allow-net --deny-write ./source/main.tsx --output ./stews"
  }
}
